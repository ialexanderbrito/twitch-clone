{
  "expo": {
    "name": "TwitchTV",
    "slug": "TwitchTV",
    "version": "1.0.0",
    "privacy": "public",
    "sdkVersion": "39.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#9147FF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
