      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.ialexanderbrito.TwitchTV"
    },
    "description": "\nClone do aplicativo de Streaming de games.",
    "githubUrl": "https://github.com/ialexanderbrito/twitch-clone"
  }
}
